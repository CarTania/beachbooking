{
    "opts": {
        "template": "node_modules/docdash"
    },
    "source": {
        "includePattern": ".+\\.tsx?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "node_modules/jsdoc-babel",
        "plugins/markdown"
    ],
    "babel": {
        "extensions": [
            "ts",
            "tsx"
        ],
        "presets": [
            "@babel/preset-env",
            "@babel/preset-typescript"
        ]
    }
}